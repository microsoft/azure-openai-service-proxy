{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Playground",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src/playground",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "preLaunchTask": "npm: install"
    },
    {
      "type": "node-terminal",
      "name": "SWA Playground",
      "request": "launch",
      "command": "swa start playground",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Management UI",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/src/AzureAIProxy.Management/AzureAIProxy.Management.csproj"
    },
    {
      "name": "AzureAIProxy",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/src/AzureAIProxy/AzureAIProxy.csproj"
    }
  ],
  "compounds": [
    {
      "name": "Run all",
      "configurations": [
        "AzureAIProxy",
        "Playground",
        "SWA Playground",
        "Management UI"
      ],
      "presentation": {
        "hidden": false,
        "group": "Frontend",
        "order": 1
      }
    },
    {
      "name": "Admin",
      "configurations": ["AzureAIProxy", "Management UI"],
      "presentation": {
        "hidden": false,
        "group": "Frontend",
        "order": 1
      }
    },
    {
      "name": "End user",
      "configurations": ["AzureAIProxy", "Playground", "SWA Playground"],
      "presentation": {
        "hidden": false,
        "group": "Frontend",
        "order": 1
      }
    }
  ]
}
